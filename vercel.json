{
  "version": 2,
  "builds": [
    {
      "src": "api/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/scrape",
      "dest": "/api/server.js"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ],
  "env": {
    "PLAYWRIGHT_BROWSERS_PATH": "0"
  },
  "build": {
    "env": {
      "PLAYWRIGHT_BROWSERS_PATH": "0"
    },
    "commands": [
      {
        "command": "bash vercel-build.sh"
      }
    ]
  }
}
